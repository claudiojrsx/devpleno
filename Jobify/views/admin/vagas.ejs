<%- include('header') %>
        <h2>
            Admin Vagas:
            <a href="/admin/vagas/nova">adicionar nova vaga</a>
        </h2>
        <div>
            <ul>
                <% vagas.forEach( vaga => { %>
                <li>
                    <%- vaga.titulo %> | 
                    <a href="/admin/vagas/delete/<%- vaga.id %>">Excluir</a>
                </li>
            <% }) %>
            </ul>
        </div>
<%- include('footer') %>
